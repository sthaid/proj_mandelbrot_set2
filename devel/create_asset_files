#!/bin/bash

if [ `basename $PWD` != devel ]; then
  echo must be in devel dir
fi

cd ..

rm -f ~/.mbs2/*.dat
rm -f ./assets/*.dat

mbs2 -c -0.750000000000000000,0.000000000000000000,0,0.000,400,2,1
mbs2 -c -0.155305495778334274,-1.027685766736389450,27,0.500,570,2,1
mbs2 -c 0.278085936329931838,-0.475664059526602789,46,0.000,400,2,1
mbs2 -c -0.030097892087744813,-0.641739594636805299,14,0.900,640,2,1
mbs2 -c 0.261990600940702922,0.002626766973205919,19,0.000,575,2,1
mbs2 -c -0.705218645810999578,-0.285260972612839392,11,0.800,400,2,1
mbs2 -c 0.261990600940702922,0.002626766973205919,46,0.000,400,2,2
mbs2 -c 0.261990600940702922,0.002626766973205919,17,0.800,575,2,1
mbs2 -c -0.155117074119472870,-1.027629953839807042,12,0.700,400,2,1
mbs2 -c -0.199236735320448916,-1.100102024904893172,16,0.300,400,2,1
mbs2 -c -0.199235212730572792,-1.100113762266798467,44,0.100,400,6,1
mbs2 -c 0.261961443659717352,0.002194267305253962,10,0.400,400,3,1
mbs2 -c -0.705218614702636803,-0.285260959362422739,26,0.400,400,2,1
mbs2 -c 0.273597562274597583,0.005876794511916007,41,0.500,510,3,1
mbs2 -c -0.155305495685645778,-1.027685764233799759,27,0.100,400,2,1
mbs2 -c -0.155305497682560661,-1.027685767144814077,25,0.400,570,2,1

cp ~/.mbs2/*.dat assets
